    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\server.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Seeder delete",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\seeder.js",
            "args": ["-d"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Seeder import",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\seeder.js",
            "args": ["-i"]
        }
    ]
}